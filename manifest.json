{
  "manifest_version": 2,
  "name": "BetterMotivation",
  "description": "Replaces LinkedIn videos with Shia LaBeouf",
  "short_name": "BetterMotivation",
  "version": "0.1.1",
  "content_scripts": [
    {
      "matches": [
        "*://linkedin.com/*",
        "*://www.linkedin.com/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}